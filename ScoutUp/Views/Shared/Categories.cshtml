@model Dictionary<ScoutUp.ViewModels.CategoryViewModel,List<ScoutUp.ViewModels.CategoryItemViewModel>>
    
@foreach (var item in Model)
{
    <div class="row" id="@String.Concat("category",@item.Key.CategoryId)"><h4>@item.Key.CategoryName</h4>
        <ul class="sidebyside">
    @foreach (var categories in item.Value)
    {
        <li>
            <div class=".col-md-1" style="margin-left:5px">
